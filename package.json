  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.21.1",
    "fast-xml-parser": "^4.0.1",
    "threads": "^0.12.1",
    "yarn": "^1.22.17"
  },
  "devDependencies": {
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "jest-mock-axios": "^4.5.0",
    "lint-staged": "^12.3.2",
    "nock": "^13.2.2",
    "prettier": "^2.5.1"
